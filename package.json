{
  "name": "@winkgroup/env",
  "version": "1.4.2",
  "description": "Utility library used in WiNK for tasks related with dotenv",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "format": "prettier --write 'src/**/*.ts'",
    "prepare": "npm run build",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WINKgroup/env.git"
  },
  "keywords": [
    "dotenv",
    "env",
    "environment variables",
    "environment"
  ],
  "author": "winksrl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WINKgroup/env/issues"
  },
  "homepage": "https://github.com/WINKgroup/env#readme",
  "dependencies": {
    "dotenv": "^16.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  }
}
